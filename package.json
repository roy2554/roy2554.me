{
  "name": "roy2554.me",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-client": "cd client && yarn build",
    "build-server": "cd server && yarn build",
    "start-client-production": "cd client && yarn start",
    "start-server-production": "cd server && yarn start",
    "start-client-development": "cd client && yarn dev",
    "start-server-development": "cd server && yarn start:dev",
    "start": "concurrently --kill-others-on-fail \"yarn start-client-production\" \"yarn start-server-production\"",
    "dev": "concurrently --kill-others-on-fail \"yarn start-client-development\" \"yarn start-server-development\"",
    "build": "yarn build-client \" \"yarn build-server"
  },
  "dependencies": {
    "nodemon": "2.0.20"
  }
}
